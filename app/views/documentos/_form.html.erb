<%= semantic_form_for(@documento) do |f| %>

  <%= f.input :tipo, :as => :select, :collection => Documento.tipos %>
  <%= f.input :assunto %>
  <%= f.input :numero_de_processo %>
  <%= f.input :setor_origem, :label_method => :nome %>
  <%= f.input :funcionario_origem, :label_method => :nome %>
  <%= f.submit 'Salvar' %>

<% end %>
